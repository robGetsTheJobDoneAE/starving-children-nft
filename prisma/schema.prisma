datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}
model Nft {
  id       String    @id @default(auto()) @map("_id") @db.ObjectId 
  tokenId  String
}

model Sale {
  id       String    @id @default(auto()) @map("_id") @db.ObjectId 
  transactionHash String @unique
  to       String
  from     String
  price    BigInt
  tokenId  String
}
